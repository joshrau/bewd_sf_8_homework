<h1>Links#new</h1>
<p>Find me in app/views/links/new.html.erb</p>

<% if @link.errors.any? %>
	<div>
		<p>error</p>
	</div>		

	<% @link.errors.full_messages.each do  |msg| %>
		<%= msg %>
	<% end %>
<% end %>	

<%= form_for(@link) do |f| %>

 <div class="field">
    <%= f.label :link %><br>
    <%= f.text_field :link%>
  </div> 
 
  <div class="actions">
    <%= f.submit %>
  </div> 

<% end %>

